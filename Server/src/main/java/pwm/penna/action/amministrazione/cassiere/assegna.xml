<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="cassiere_assegna" namespace="/Cassiere" extends="cassiere">
		<action name="Assegna" class="pwm.penna.action.amministrazione.cassiere.AssegnaInserisci">
			<result>/WEB-INF/Amministrazione/cassiere.jsp</result>
		</action>
		
		<action name="Crea" class="pwm.penna.action.amministrazione.cassiere.AssegnaCrea">
			<result name="success" type="redirectAction">Assegna</result>
			<result name="input">/WEB-INF/Amministrazione/cassiere.jsp</result>
		</action>
		
		<action name="">
			<result type="redirectAction">Assegna</result>
		</action>
	</package>
</struts>
