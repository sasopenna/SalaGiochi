<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="dispositivodigioco" namespace="/Dirigenza/DispositivoDiGioco" extends="dirigente">
        <action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.inserisci.DispositivoDiGiocoInserisci">
            <result>/WEB-INF/Amministrazione/Dirigenza/DispositiviDiGioco/Dispositivo/inserisci.jsp</result>
        </action>
        
        <action name="Crea" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.inserisci.DispositivoDiGiocoCrea">
            <result name="success" type="redirectAction">Lista</result>
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/DispositiviDiGioco/Dispositivo/inserisci.jsp</result>
        </action>
        
        <action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.modifica.DispositivoDiGiocoModifica">
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/DispositiviDiGioco/Dispositivo/modifica.jsp</result>
        </action>
        
        <action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.modifica.DispositivoDiGiocoAggiorna">
            <result name="success" type="redirectAction">Lista</result>
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/DispositiviDiGioco/Dispositivo/modifica.jsp</result>
        </action>
        
        <action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.cancella.DispositivoDiGiocoCancella">
            <result name="success" type="redirectAction">Lista</result>
        </action>
        
        <action name="Lista" class="pwm.penna.action.amministrazione.dirigente.dispositivodigioco.ddg.lista.DispositivoDiGiocoLista">
            <result>/WEB-INF/Amministrazione/Dirigenza/DispositiviDiGioco/Dispositivo/visualizza.jsp</result>
        </action>
        
        <action name="">
            <result type="redirectAction">Lista</result>
        </action>
    </package>
</struts>
