<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="assegnazioni" namespace="/Dirigenza/Postazione/Assegna" extends="dirigente">
		<action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.inserisci.AssegnaInserisci">
			<result>/WEB-INF/Amministrazione/Dirigenza/Postazioni/Assegna/inserisci.jsp</result>
		</action>
		
		<action name="Crea" class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.inserisci.AssegnaCrea">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Assegna/inserisci.jsp</result>
		</action>
		
		<action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.modifica.AssegnaModifica">
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Assegna/modifica.jsp</result>
		</action>
		
		<action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.modifica.AssegnaAggiorna">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Assegna/modifica.jsp</result>
		</action>
		
		<action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.cancella.AssegnaCancella">
			<result name="success" type="redirectAction">Lista</result>
		</action>
		
		<action name="Lista"  class="pwm.penna.action.amministrazione.dirigente.postazioni.assegna.lista.AssegnaLista">
			<result>/WEB-INF/Amministrazione/Dirigenza/Postazioni/Assegna/visualizza.jsp</result>
		</action>
		
		<action name="">
			<result type="redirectAction">Lista</result>
		</action>
	</package>
</struts>
