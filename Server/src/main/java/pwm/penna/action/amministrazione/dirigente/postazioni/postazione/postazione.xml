<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="postazioni" namespace="/Dirigenza/Postazione" extends="dirigente">
		<action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.inserisci.PostazioneInserisci">
			<result>/WEB-INF/Amministrazione/Dirigenza/Postazioni/Postazione/inserisci.jsp</result>
		</action>
		
		<action name="Crea" class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.inserisci.PostazioneCrea">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Postazione/inserisci.jsp</result>
		</action>
		
		<action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.modifica.PostazioneModifica">
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Postazione/modifica.jsp</result>
		</action>
		
		<action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.modifica.PostazioneAggiorna">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Postazioni/Postazione/modifica.jsp</result>
		</action>
		
		<action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.cancella.PostazioneCancella">
			<result name="success" type="redirectAction">Lista</result>
		</action>
		
		<action name="Lista"  class="pwm.penna.action.amministrazione.dirigente.postazioni.postazione.lista.PostazioneLista">
			<result>/WEB-INF/Amministrazione/Dirigenza/Postazioni/Postazione/visualizza.jsp</result>
		</action>
		
		<action name="">
			<result type="redirectAction">Lista</result>
		</action>
	</package>
</struts>
