<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="cassieri" namespace="/Dirigenza/Dipendenti/Cassiere" extends="dirigente">
        <action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.inserisci.CassiereInserisci">
            <result>/WEB-INF/Amministrazione/Dirigenza/Dipendenti/Cassiere/inserisci.jsp</result>
        </action>
        
        <action name="Crea" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.inserisci.CassiereCrea">
            <result name="success" type="redirectAction">Lista</result>
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/Dipendenti/Cassiere/inserisci.jsp</result>
        </action>
        
        <action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.modifica.CassiereModifica">
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/Dipendenti/Cassiere/modifica.jsp</result>
        </action>
        
        <action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.modifica.CassiereAggiorna">
            <result name="success" type="redirectAction">Lista</result>
            <result name="input">/WEB-INF/Amministrazione/Dirigenza/Dipendenti/Cassiere/modifica.jsp</result>
        </action>
        
        <action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.cancella.CassiereCancella">
            <result name="success" type="redirectAction">Lista</result>
        </action>
        
        <action name="Lista" class="pwm.penna.action.amministrazione.dirigente.dipendenti.cassiere.lista.CassiereLista">
            <result>/WEB-INF/Amministrazione/Dirigenza/Dipendenti/Cassiere/visualizza.jsp</result>
        </action>
        
        <action name="">
            <result type="redirectAction">Lista</result>
        </action>
    </package>
</struts>
