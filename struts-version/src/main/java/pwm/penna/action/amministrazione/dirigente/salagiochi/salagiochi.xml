<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="salagiochi" namespace="/Dirigenza/SalaGiochi" extends="dirigente">
		<action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.salagiochi.inserisci.SalaGiochiInserisci">
			<result>/WEB-INF/Amministrazione/Dirigenza/SalaGiochi/inserisci.jsp</result>
		</action>
		
		<action name="Crea" class="pwm.penna.action.amministrazione.dirigente.salagiochi.inserisci.SalaGiochiCrea">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/SalaGiochi/inserisci.jsp</result>
		</action>
		
		<action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.salagiochi.modifica.SalaGiochiModifica">
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/SalaGiochi/modifica.jsp</result>
		</action>
		
		<action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.salagiochi.modifica.SalaGiochiAggiorna">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/SalaGiochi/modifica.jsp</result>
		</action>
		
		<action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.salagiochi.cancella.SalaGiochiCancella">
			<result name="success" type="redirectAction">Lista</result>
		</action>
		
		<action name="Lista"  class="pwm.penna.action.amministrazione.dirigente.salagiochi.lista.SalaGiochiLista">
			<result>/WEB-INF/Amministrazione/Dirigenza/SalaGiochi/visualizza.jsp</result>
		</action>
		
		<action name="">
			<result type="redirectAction">Lista</result>
		</action>
	</package>
</struts>
