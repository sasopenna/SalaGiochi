<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="soci" namespace="/Dirigenza/Socio" extends="dirigente">
    	<action name="Inserisci" class="pwm.penna.action.amministrazione.dirigente.socio.inserisci.SocioInserisci">
			<result>/WEB-INF/Amministrazione/Dirigenza/Socio/inserisci.jsp</result>
		</action>
		
		<action name="Crea" class="pwm.penna.action.amministrazione.dirigente.socio.inserisci.SocioCrea">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Socio/inserisci.jsp</result>
		</action>
		
		<action name="Modifica" class="pwm.penna.action.amministrazione.dirigente.socio.modifica.SocioModifica">
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Socio/modifica.jsp</result>
		</action>
		
		<action name="Aggiorna" class="pwm.penna.action.amministrazione.dirigente.socio.modifica.SocioAggiorna">
			<result name="success" type="redirectAction">Lista</result>
			<result name="input">/WEB-INF/Amministrazione/Dirigenza/Socio/modifica.jsp</result>
		</action>
		
		<action name="Cancella" class="pwm.penna.action.amministrazione.dirigente.socio.cancella.SocioCancella">
			<result name="success" type="redirectAction">Lista</result>
		</action>
		
		<action name="Lista"  class="pwm.penna.action.amministrazione.dirigente.socio.lista.SocioLista">
			<result>/WEB-INF/Amministrazione/Dirigenza/Socio/visualizza.jsp</result>
		</action>
		
		<action name="">
			<result type="redirectAction">Lista</result>
		</action>
	</package>
</struts>
